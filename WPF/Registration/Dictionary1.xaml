<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Registration">
    <Style x:Key="textBox">
        <Setter Property="Control.FontFamily" Value="Calibri Light"/>
        <Setter Property="Control.Margin" Value="50,5"/>
        <Setter Property="Control.Height" Value="40"/>
        <Setter Property="Control.FontSize" Value="17"/>
    </Style>
    <Style x:Key="signInBtn" TargetType="Button">
        <Setter Property="Height" Value="30"/>
        <Setter Property="Margin" Value="50,10"/>
        <Setter Property="Background" Value="#FF41B13A"/>
        <Setter Property="Content" Value="SIGN IN"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>
</ResourceDictionary>